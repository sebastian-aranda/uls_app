<?xml version='1.0' encoding='utf-8'?>
<widget id="cl.milenium.uls" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ULS2016</name>
    <description>
        Feria del Software 2016 - Ultimate Laser Shot
    </description>
    <author email="aranda.sebastian@gmail.com" href="http://sebastian-aranda.github.io/">
        Sebastian Aranda Sanchez
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-vibration" spec="~2.1.2" />
    <plugin name="cordova-plugin-websocket" spec="~0.12.0" />
    <plugin name="cordova-plugin-barcodescanner" spec="~0.7.0" />
    <preference name="Orientation" value="landscape" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="android" spec="~6.0.0" />
</widget>
